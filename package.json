  "scripts": {
    "audit-ci": "audit-ci --high -p yarn",
    "start": "NODE_ENV=development webpack-dev-server --open --config webpack.dev.js",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "lint": "eslint app/src/js/ --ext .js",
    "cypress": "CYPRESS_TESTING=true cypress open",
    "cypress-ci": "CYPRESS_TESTING=true cypress run --spec 'cypress/integration/*' --config video=false",
    "validation": "ava cypress/validation-tests/*.js",
    "test": "npm run lint && ava"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/fake-api/**/*",
      "!test/fixtures/**/*"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
